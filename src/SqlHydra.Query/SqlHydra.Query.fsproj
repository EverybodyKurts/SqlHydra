<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <WarnOn>3390;$(WarnOn)</WarnOn>
    <Version>0.1.0</Version>
    <Description>SqlHydra.Katas is a set of F# query builders powered by SqlKata.</Description>
    <Authors>Jordan Marr</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="LinqExpressionVisitors.fs" />
    <Compile Include="KataBuilders.fs" />
    <Compile Include="Context.fs" />
    <Compile Include="Extensions.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SqlKata" Version="2.3.7" />
    <PackageReference Include="TaskBuilder.fs" Version="2.1.0" />
  </ItemGroup>

</Project>
